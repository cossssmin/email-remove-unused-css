{
  "name": "email-remove-unused-css",
  "version": "2.7.2",
  "description": "Remove unused CSS from email templates",
  "main": "index.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-dir es5",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepublish": "npm run build",
    "test": "standard && nyc --reporter=html --reporter=text ava"
  },
  "repository": "codsen/email-remove-unused-css",
  "keywords": [
    "email",
    "remove",
    "unused",
    "css",
    "head",
    "styles",
    "body",
    "uncss",
    "inline",
    "from"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "https://github.com/revelt"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/email-remove-unused-css/issues"
  },
  "homepage": "https://github.com/codsen/email-remove-unused-css",
  "dependencies": {
    "array-pull-all-with-glob": "^3.0.2",
    "lodash.intersection": "*",
    "lodash.isplainobject": "*",
    "lodash.pullall": "*",
    "lodash.uniq": "*",
    "string-extract-class-names": "^3.4.0",
    "string-replace-slices-array": "^1.2.0",
    "string-slices-array-push": "^1.2.0"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "*",
    "husky": "*",
    "nyc": "*",
    "standard": "^10.0.3"
  },
  "ava": {
    "verbose": true
  },
  "standard": {
    "ignore": [
      "es5/"
    ]
  }
}
